-- Fungsi loading box dengan animasi
function showLoading(text, duration)
    local loadingBox = getLoadingBox(text)
    if loadingBox then
        loadingBox["æ˜¾ç¤º"]()
        if duration then
            gg.sleep(duration)
            loadingBox["å…³é—­"]()
        end
        return loadingBox
    end
    return nil
end

-- Animasi loading bertahap
function stepLoading(steps)
    for i, step in ipairs(steps) do
        local loading = showLoading(step.text)
        gg.sleep(step.duration or 1000)
        if loading then
            loading["å…³é—­"]()
        end
    end
end

-- File untuk menyimpan cloud custom
local CUSTOM_MUSIC_FILE = gg.EXT_STORAGE .. "/custom_music_list.lua"

-- Fungsi untuk memeriksa apakah file exists
function fileExists(file)
    local f = io.open(file, "r")
    if f then
        f:close()
        return true
    end
    return false
end

-- Fungsi untuk memuat cloud custom dari file
function loadCustomMusic()
    local customMusic = {}
    
    local success, fileContent = pcall(function()
        local f = io.open(CUSTOM_MUSIC_FILE, "r")
        if f then
            local content = f:read("*a")
            f:close()
            return content
        end
        return nil
    end)
    
    if success and fileContent and fileContent ~= "" then
        for name, url in fileContent:gmatch('name = "([^"]+)", url = "([^"]+)"') do
            table.insert(customMusic, {name = name, url = url})
        end
    end
    
    return customMusic
end

-- Fungsi untuk menyimpan cloud custom ke file
function saveCustomMusic(customMusic)
    local success, result = pcall(function()
        local file = io.open(CUSTOM_MUSIC_FILE, "w")
        if file then
            file:write("-- Custom Music Cloud Storage\n")
            file:write("-- Created by Vellix Music Player\n\n")
            for i, song in ipairs(customMusic) do
                file:write('name = "' .. song.name .. '", url = "' .. song.url .. '"\n')
            end
            file:close()
            return true
        end
        return false
    end)
    
    return success and result
end

-- Fungsi untuk menambah lagu ke cloud custom
function addToCustomMusic()
    stepLoading({
        {text = "ğŸ“ Membuka editor...", duration = 600},
        {text = "ğŸµ Menyiapkan form...", duration = 500}
    })
    
    local input = gg.prompt({
        "ğŸµ NAMA LAGU:",
        "ğŸ”— LINK MUSIK:"
    }, {
        [1] = "",
        [2] = ""
    }, {
        [1] = "text",
        [2] = "text"
    })
    
    if input == nil then
        gg.colorToast("<font color='#FF6B6B'>âŒ Dibatalkan</font>")
        return
    end
    
    if input[1] == "" or input[2] == "" then
        gg.colorAlert(
            "<font color='#FF6B6B'>âš ï¸ DATA KOSONG</font>",
            "<font color='#FFA500'>Nama lagu dan link tidak boleh kosong!</font>\n\n" ..
            "<font color='#98D8C8'>ğŸ“‹ Pastikan mengisi kedua field</font>",
            "ğŸ”„ COBA LAGI", "", "â“ BANTUAN"
        )
        return
    end
    
    if not (string.find(input[2], "https://") or string.find(input[2], "http://")) then
        gg.colorAlert(
            "<font color='#FF4444'>âŒ LINK TIDAK VALID</font>",
            "<font color='#FFB6C1'>Format link tidak sesuai!</font>\n\n" ..
            "<font color='#98D8C8'>ğŸ”— Link harus dimulai dengan:</font>\n" ..
            "<font color='#90EE90'>âœ“ https://</font>\n" ..
            "<font color='#90EE90'>âœ“ http://</font>",
            "ğŸ”™ KEMBALI", "", "ğŸ“– CONTOH"
        )
        return
    end
    
    local customMusic = loadCustomMusic()
    
    local existingIndex = nil
    for i, song in ipairs(customMusic) do
        if song.name == input[1] then
            existingIndex = i
            break
        end
    end
    
    if existingIndex then
        local confirm = gg.alert(
            "âš ï¸ LAGU SUDAH ADA\n\n" ..
            "Lagu dengan nama '" .. input[1] .. "' sudah ada!\n" ..
            "Apakah ingin menggantinya?",
            "âœ… GANTI", "âŒ BATAL", "âœï¸ EDIT NAMA"
        )
        
        if confirm == 1 then
            customMusic[existingIndex] = {name = input[1], url = input[2]}
        elseif confirm == 3 then
            local newName = gg.prompt("âœï¸ NAMA BARU:", {[1] = input[1]}, {[1] = "text"})
            if newName and newName[1] ~= "" then
                input[1] = newName[1]
                customMusic[existingIndex] = {name = input[1], url = input[2]}
            else
                return
            end
        else
            return
        end
    else
        table.insert(customMusic, {
            name = input[1],
            url = input[2]
        })
    end
    
    if saveCustomMusic(customMusic) then
        showLoading("ğŸ’¾ Menyimpan ke cloud...", 1000)
        gg.colorAlert(
            "<font color='#00FA9A'>âœ… BERHASIL</font>",
            "<font color='#98FB98'>ğŸµ Lagu berhasil ditambahkan!</font>\n\n" ..
            "<font color='#FFD700'>ğŸ“ Nama:</font>\n" ..
            "<font color='#87CEEB'>" .. input[1] .. "</font>\n\n" ..
            "<font color='#FFB6C1'>ğŸ”— Link tersimpan di cloud</font>",
            "ğŸµ OK", "â• TAMBAH LAGI", "â˜ï¸ LIHAT CLOUD"
        )
    else
        gg.colorAlert(
            "<font color='#FF4444'>âŒ GAGAL</font>",
            "<font color='#FF6B6B'>Gagal menyimpan lagu!</font>\n\n" ..
            "<font color='#B0E0E6'>Coba lagi atau periksa penyimpanan</font>",
            "ğŸ”„ COBA LAGI", "âŒ BATAL", ""
        )
    end
end

-- Fungsi untuk melihat dan memutar cloud custom
function viewCustomMusic()
    local customMusic = loadCustomMusic()
    
    if #customMusic == 0 then
        gg.colorAlert(
            "<font color='#FFA500'>â˜ï¸ CLOUD KOSONG</font>",
            "<font color='#FFD700'>Belum ada lagu di cloud custom!</font>\n\n" ..
            "<font color='#98D8C8'>ğŸ’¡ Tambahkan lagu dulu di menu Cloud Custom</font>",
            "â• TAMBAH LAGU", "âŒ KELUAR", "â„¹ï¸ INFO"
        )
        return
    end
    
    while true do
        local menuItems = {}
        for i, song in ipairs(customMusic) do
            table.insert(menuItems, "ğŸµ " .. song.name)
        end
        table.insert(menuItems, "â¹ï¸ STOP MUSIC")
        table.insert(menuItems, "ğŸ”™ KEMBALI")
        
        local choice = gg.choice(menuItems, nil, "â˜ï¸ CLOUD CUSTOM ANDA\n\nğŸ“š Total " .. #customMusic .. " lagu tersimpan\nğŸµ Pilih lagu yang ingin diputar")
        
        if choice and choice <= #customMusic then
            local song = customMusic[choice]
            local loading = showLoading("ğŸ¶ Memuat " .. song.name .. "...")
            gg.playMusic(song.url)
            if loading then loading["å…³é—­"]() end
            
            showLoading("âœ… " .. song.name .. " diputar!", 1000)
            gg.colorToast("<font color='#87CEEB'>ğŸµ Memutar: " .. song.name .. "</font>")
            
        elseif choice == #menuItems - 1 then
            local loading = showLoading("â¹ï¸ Menghentikan musik...")
            gg.playMusic("stop")
            if loading then loading["å…³é—­"]() end
            gg.colorToast("<font color='#FF6B6B'>ğŸ›‘ Musik dihentikan</font>")
            
        elseif choice == #menuItems or choice == nil then
            break
        end
    end
end

-- Fungsi untuk mengelola cloud custom (edit/hapus)
function manageCustomMusic()
    local customMusic = loadCustomMusic()
    
    if #customMusic == 0 then
        gg.colorAlert(
            "<font color='#FFA500'>ğŸ“ TIDAK ADA LAGU</font>",
            "<font color='#FFD700'>Belum ada lagu untuk dikelola!</font>\n\n" ..
            "<font color='#98D8C8'>ğŸ’¡ Tambahkan lagu dulu di menu Cloud Custom</font>",
            "â• TAMBAH LAGU", "âŒ KELUAR", ""
        )
        return
    end
    
    while true do
        local menuItems = {}
        for i, song in ipairs(customMusic) do
            table.insert(menuItems, "ğŸµ " .. song.name)
        end
        table.insert(menuItems, "ğŸ”™ KEMBALI")
        
        local choice = gg.choice(menuItems, nil, "âš™ï¸ KELOLA CLOUD CUSTOM\n\nğŸ“ Pilih lagu yang ingin dikelola\nâœï¸ Edit atau hapus lagu")
        
        if choice and choice <= #customMusic then
            local song = customMusic[choice]
            
            local action = gg.choice({
                "âœï¸ EDIT LAGU",
                "ğŸ—‘ï¸ HAPUS LAGU", 
                "â–¶ï¸ PUTAR LAGU",
                "âŒ BATAL"
            }, nil, "ğŸµ KELOLA: " .. song.name .. "\n\nPilih aksi yang diinginkan")
            
            if action == 1 then
                local input = gg.prompt({
                    "âœï¸ NAMA LAGU BARU:",
                    "ğŸ”— LINK BARU:"
                }, {
                    [1] = song.name,
                    [2] = song.url
                }, {
                    [1] = "text",
                    [2] = "text"
                })
                
                if input and input[1] ~= "" and input[2] ~= "" then
                    customMusic[choice].name = input[1]
                    customMusic[choice].url = input[2]
                    if saveCustomMusic(customMusic) then
                        gg.colorToast("<font color='#00FA9A'>âœ… Lagu berhasil diupdate!</font>")
                    end
                end
                
            elseif action == 2 then
                local confirm = gg.alert(
                    "ğŸ—‘ï¸ HAPUS LAGU\n\n" ..
                    "Apakah yakin ingin menghapus?\n\n" ..
                    "ğŸµ " .. song.name .. "\n\n" ..
                    "Tindakan ini tidak dapat dibatalkan!",
                    "âœ… HAPUS", "âŒ BATAL"
                )
                
                if confirm == 1 then
                    table.remove(customMusic, choice)
                    if saveCustomMusic(customMusic) then
                        gg.colorToast("<font color='#00FA9A'>âœ… Lagu berhasil dihapus!</font>")
                        break
                    end
                end
                
            elseif action == 3 then
                local loading = showLoading("ğŸ¶ Memutar " .. song.name .. "...")
                gg.playMusic(song.url)
                if loading then loading["å…³é—­"]() end
                gg.colorToast("<font color='#87CEEB'>ğŸµ Memutar: " .. song.name .. "</font>")
            end
            
        elseif choice == #menuItems or choice == nil then
            break
        end
    end
end

-- Menu Cloud Custom
function cloudCustomMenu()
    while true do
        local customMusic = loadCustomMusic()
        local totalSongs = #customMusic
        
        local menuItems = {
            "â• TAMBAH LAGU BARU",
            "â˜ï¸ LIHAT & PUTAR LAGU", 
            "âš™ï¸ KELOLA LAGU SAYA",
            "ğŸ“Š INFO CLOUD",
            "ğŸ”™ KEMBALI"
        }
        
        local menu = gg.choice(menuItems, nil, "â˜ï¸ CLOUD CUSTOM\n\nğŸ’¾ Penyimpanan playlist pribadi\nğŸµ Total lagu: " .. totalSongs .. " lagu")
        
        if menu == 1 then
            addToCustomMusic()
        elseif menu == 2 then
            viewCustomMusic()
        elseif menu == 3 then
            manageCustomMusic()
        elseif menu == 4 then
            gg.colorAlert(
                "<font color='#87CEEB'>ğŸ“Š INFO CLOUD CUSTOM</font>",
                "<font color='#FFD700'>â˜ï¸ Fitur Cloud Custom</font>\n\n" ..
                "<font color='#98FB98'>âœ… Total lagu: " .. totalSongs .. "</font>\n" ..
                "<font color='#90EE90'>âœ… Tersimpan permanen</font>\n" ..
                "<font color='#90EE90'>âœ… Buka kapan saja</font>\n" ..
                "<font color='#90EE90'>âœ… Edit dan hapus</font>\n\n" ..
                "<font color='#FFB6C1'>ğŸ’¡ Simpan playlist favoritmu!</font>",
                "ğŸµ OK", "â• TAMBAH LAGU", "â˜ï¸ LIHAT LAGU"
            )
        elseif menu == 5 or menu == nil then
            break
        end
    end
end

-- Fungsi untuk memutar musik dari link custom
function playMusicFromLink()
    stepLoading({
        {text = "ğŸŒŸ Membuka panel input...", duration = 700},
        {text = "ğŸ§ Menyiapkan player...", duration = 600},
        {text = "âœ¨ Menunggu link musik...", duration = 500}
    })
    
    local link = gg.prompt({
        "ğŸµ MASUKKAN LINK MUSIK:"
    }, {
        [1] = ""
    }, {
        [1] = "text"
    })
    
    if link == nil then
        gg.colorToast("<font color='#FF6B6B'>âŒ Dibatalkan oleh pengguna</font>")
        return
    end
    
    if link[1] == "" then
        showLoading("âš ï¸ Link kosong terdeteksi...", 1000)
        gg.colorAlert(
            "<font color='#FF6B6B'>âš ï¸ PERINGATAN</font>",
            "<font color='#FFA500'>Link tidak boleh kosong!</font>\n\n" ..
            "<font color='#98D8C8'>ğŸ’¡ Masukkan link musik yang valid</font>",
            "ğŸ”„ COBA LAGI", "", "â“ BANTUAN"
        )
        return
    end
    
    local musicLink = link[1]
    
    if not (string.find(musicLink, "https://") or string.find(musicLink, "http://")) then
        showLoading("âŒ Validasi gagal...", 1000)
        gg.colorAlert(
            "<font color='#FF4444'>âŒ FORMAT ERROR</font>",
            "<font color='#FFB6C1'>Format link tidak valid!</font>\n\n" ..
            "<font color='#98D8C8'>ğŸ“‹ Link harus dimulai dengan:</font>\n" ..
            "<font color='#90EE90'>âœ“ https://</font>\n" ..
            "<font color='#90EE90'>âœ“ http://</font>",
            "ğŸ”™ KEMBALI", "", "ğŸ“– BANTUAN"
        )
        return
    end
    
    gg.colorAlert(
        "<font color='#98FB98'>ğŸµ KONFIRMASI</font>",
        "<font color='#FFD700'>ğŸ“ Link musik:</font>\n" ..
        "<font color='#B0E0E6'>" .. musicLink .. "</font>\n\n" ..
        "<font color='#FFB6C1'>ğŸ§ Siap untuk diputar!</font>",
        "â–¶ï¸ PUTAR", "âŒ BATAL", "ğŸ”‡ MUTE"
    )
    
    local loading = showLoading("ğŸ¶ Memuat musik...")
    gg.playMusic(musicLink)
    if loading then loading["å…³é—­"]() end
    
    showLoading("âœ… Musik diputar!", 1200)
    
    gg.colorAlert(
        "<font color='#00FA9A'>âœ¨ BERHASIL</font>",
        "<font color='#98FB98'>ğŸ‰ Musik berhasil diputar!</font>\n\n" ..
        "<font color='#FFD700'>ğŸ”— Link aktif:</font>\n" ..
        "<font color='#87CEEB'>" .. musicLink .. "</font>\n\n" ..
        "<font color='#FFB6C1'>ğŸ¶ Nikmati musiknya! âœ¨</font>",
        "ğŸµ OK", "ğŸ” GANTI", "â¹ï¸ STOP"
    )
end

