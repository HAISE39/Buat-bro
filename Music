-- Menu utama
function main()
    while true do
        local menuItems = {
            "ğŸµ PUTAR DARI LINK",
            "â˜ï¸ MUSIC CLOUD", 
            "â­ CLOUD CUSTOM",
            "â¹ï¸ HENTIKAN MUSIK",
            "ğŸ“± HUBUNGI DEVELOPER",
            "âŒ KELUAR"
        }
        
        local menu = gg.choice(menuItems, nil, "âœ¨ GAME GUARDIAN MUSIC PLAYER âœ¨\n\nğŸ¼ Menu Utama v3.0\nğŸ“š Pilih menu yang tersedia")
        
        if menu == 1 then
            playMusicFromLink()
        elseif menu == 2 then
            musicCloud()
        elseif menu == 3 then
            cloudCustomMenu()
        elseif menu == 4 then
            stopMusic()
        elseif menu == 5 then
            contactDeveloper()
        elseif menu == 6 or menu == nil then
            stepLoading({
                {text = "ğŸ’¾ Menyimpan preferensi...", duration = 700},
