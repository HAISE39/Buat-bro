-- Fungsi stop musik
function stopMusic()
    local loading = showLoading("â¹ï¸ Menghentikan musik...")
    gg.playMusic("stop")
    if loading then loading["å…³é—­"]() end
    
    gg.colorAlert(
        "<font color='#FF6B6B'>â¹ï¸ MUSIK DIHENTIKAN</font>",
        "<font color='#FFB6C1'>ğŸµ Pemutaran musik dihentikan</font>\n\n" ..
        "<font color='#98FB98'>âœ“ Berhasil</font>",
        "âœ… OK", "", ""
    )
    
    gg.colorToast("<font color='#FF6B6B'>ğŸ›‘ Musik dihentikan</font>")
end

