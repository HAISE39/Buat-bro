					gg.processResume()
					gg.toast("BUKA ENCHANTMENT")
					gg.sleep(5000)
					gg.refineNumber("-1;59;-1:9", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
